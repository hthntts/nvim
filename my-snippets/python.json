{
  "head": {
    "prefix": "head",
    "body": [
      "\"\"\"",
      "Description: $1",
      "Author: Hai Ho (hothienhaidl@gmail.com)",
      "Created: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}",
      "\"\"\"",
      "$0"
    ]
  },
  "import": {
    "prefix": "im",
    "body": "import ${1:time}"
  },
  "import as": {
    "prefix": "impa",
    "body": "import ${1:pandas} as ${2:pd}"
  },
  "main if": {
    "prefix": "main",
    "body": [
      "def main():",
      "    $1",
      "",
      "",
      "if __name__ == \"__main__\":",
      "    main()"
    ]
  },
  "printf": {
    "prefix": "prf",
    "body": "print(f'${2:$1}: {${1:expression}}')$0"
  },
  "print": {
    "prefix": "prt",
    "body": "print(${1:expression})$0"
  }
}

